@model SliderDetailVM

<div class="container">
    <div class="table-responsive table--no-card m-b-30">
        <table class="table table-borderless table-striped table-earning">
            <thead>
                <tr>
                    <th>Image</th>
                </tr>
            </thead>
            <tbody>
                <tr>
                    <td><img src="~/assets/images/@Model.SliderImage" style="border: 1px solid black" /></td>
                </tr>
            </tbody>
        </table>
    </div>
    <a asp-action="Index" asp-controller="Slider" class="btn btn-danger">Back</a>

</div>

